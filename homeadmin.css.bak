body  
{  
    margin: 0;  
    padding: 0;  
	background-color:white;
    background-image: url('mountainbg.jpg');  
	background-repeat: no-repeat;
	background-attachment: fixed;
    background-size: cover;
	font-family: 'Open Sans', sans-serif; 
}  
.left-panel{  
		text-align: left;
        width: 23.5%;  
		height: 100%;
        padding-left: 10px; 
		padding-right: 10px; 
        background-image: linear-gradient(#323232 , #696969);
        border: 0px solid black;	 
		margin: 0px;
		float:left;
		display:inline;
}  
.createAcc-panel{  
		text-align: left;
        width: 73.8%;  
		height: 100%;
        padding-left: 10px; 
		padding-right: 10px; 
        background-image: linear-gradient(to right, white , #696969);
        border: 0px solid black;	 
		margin: 0px;
		float:right;
		display:inline;
}  
h1.title {
	text-align: center;  
